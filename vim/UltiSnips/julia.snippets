snippet #!
#! /usr/bin/julia

$0
endsnippet

snippet fu
function ${1:\$name}(${2:\$args})
	${3:\$body}
end

$0
endsnippet

snippet mo
module ${1:\$name}
${2:\$body}
end
endsnippet

snippet ty
type ${1:\$name}
	${2:\$body}
end
endsnippet

snippet im
import ${1:\$what}
endsnippet

snippet ex
export ${1:\$what}
endsnippet

snippet us
using ${1:\$what}
endsnippet
